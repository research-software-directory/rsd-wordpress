{
  "env": {
    "browser": true,
    "jquery": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@wordpress/eslint-plugin/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": [
    "compat"
  ],
  "rules": {
    "no-console": "off",
    "compat/compat": "error",
    "prettier/prettier": "warn",
    "indent": [
      "error",
      "tab"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  },
  "globals": {
    "jQuery": "readonly",
    "$": "readonly",
    "rsdWordPressVars": "readonly"
  }
}
